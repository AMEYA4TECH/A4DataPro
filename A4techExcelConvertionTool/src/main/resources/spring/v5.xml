	<bean id="excelFactoryIdV5" class="com.a4tech.v5.core.excelMapping.ExcelFactory">
     	<property name="sageExcelMappingV5" ref="sageMappingIdV5" />
     </bean>
	
	
	<bean id="sageMappingIdV5" name="sageMappingV5Obj" class="com.a4tech.v5.sage.product.mapping.SageProductsExcelMapping">
     	<property name="postServiceImplV5" ref="postServiceIdV5"></property>
     	<property name="catalogParser" ref="catalogParserId"/>
     	<property name="priceGridParser" ref="pricegridParserV5"/>
     	<property name="imprintMethodParser" ref="imprintMethodId"/>
     	<property name="originParser" ref="originParserId"/>
     	<property name="rushTimeParser" ref="rushTimeId"/>
     	<property name="packagingParser" ref="packageId"/>
     	<property name="shippingEstimateParser" ref="shippingEstimateId"/>
     	<property name="dimParserObj" ref="dimensionParserId"/>
     	<property name="colorParserObj" ref="colorParserId"/>
     	<property name="productDaoObjV5"  ref="productDaoV5"/>
     </bean>
         
     <bean id="catalogParserId" class="com.a4tech.v5.sage.product.parser.CatalogParser"/>
     <!-- <bean id="priceGridParserId" class="com.a4tech.sage.product.parser.PriceGridParser"/> -->
     <bean id="imprintMethodId"  class="com.a4tech.v5.sage.product.parser.ImprintMethodParser"/>
     <bean id="originParserId" class="com.a4tech.v5.sage.product.parser.OriginParser"/>
     <bean id="rushTimeId" class="com.a4tech.v5.sage.product.parser.RushTimeParser"/>
     <bean id="packageId" class="com.a4tech.v5.sage.product.parser.PackagingParser"/>
     <bean id="shippingEstimateId" class="com.a4tech.v5.sage.product.parser.ShippingEstimateParser"/>
     <bean id="dimensionParserId" class="com.a4tech.v5.sage.product.parser.DimensionParser"/>
     <bean id="colorParserId" class="com.a4tech.v5.sage.product.parser.ColorParser"/> 
