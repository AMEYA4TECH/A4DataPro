   <bean id="productDaoV5" name="productDaoV5"
		class="com.a4tech.v5.product.dao.service.ProductDao">
		<property name="sessionFactory" ref="hibernate3AnnotatedSessionFactory"></property>
		<property name="errorFileLocPath" value="${error.logfile.location}"></property>
	</bean>
	<bean id="productV5" name="productV5Obj" class="com.a4tech.v5.product.model.Product" />
	 <bean id="lookupRestServiceIdV5" class="com.a4tech.v5.lookup.service.LookupServiceData">
   		<property name="lookupRestServiceV5" ref="lookupRestServiceIdV5"/>
   </bean>
    <bean id="lookupDataIdV5" class="com.a4tech.v5.util.LookupData">
    	<property name="lookupServiceDataV5" ref="lookupServiceDataV5"/>
    </bean>
	<bean id="postServiceIdV5" class="com.a4tech.v5.product.service.postImpl.PostServiceImpl">
		<property name="restTemplate" ref="restTemplate"></property>
		<property name="productDaoObjV5" ref="productDaoV5"></property>
		<property name="postApiURL" value="${product.v5.post.URL}"/>
		<property name="getProductUrl" value="${product.v5.get.URL}"/>
	</bean>
	<bean id="objectMapperV5" name="objectMapperV5"
		class="com.fasterxml.jackson.databind.ObjectMapper" autowire="byName"/>
<bean id="loginServiceImplV5" name="loginServiceImplV5" class="com.a4tech.v5.service.loginImpl.LoginServiceImpl"
		autowire="byName">
		<property name="restTemplate" ref="restTemplate" />
		<property name="loginApiURL" value="${login.v5.endpoint.URL}" />
	</bean>
   <bean id="lookupRestServiceV5" class="com.a4tech.v5.lookup.service.restService.LookupRestService">
   		<property name="restTemplate"        ref="restTemplate"/>
   		<property name="imprintMethodUrl"    value="${imprintMethod.v5.lookup.url}"/>
   		<property name="materialLookupUrl"   value="${materials.v5.lookup.url}"/>
   		<property name="shapesLookupUrl"     value="${shapes.v5.lookup.url}"/>
   		<property name="originLookupUrl"     value="${origin.v5.lookup.url}"/>
   		<property name="fobPointLookupUrl"   value="${fobPoint.v5.lookup.url}"/>
   		<property name="lineNamesLookupUrl"  value="${lineName.v5.lookup.url}"/>
   		<property name="tradeNameLookupUrl"  value="${tradeName.v5.lookup.url}"/>
   		<property name="categoriesLookupUrl" value="${categories.v5.lookup.url}"/>
   		<property name="catalogLookupUrl"    value="${catalog.v5.lookup.url}"/>
   	    <property name="themeLookupUrl"      value="${themes.v5.lookup.url}"/>
   	    <property name="packagesLookupUrl"   value="${packages.v5.lookup.url}"/>
   </bean>

	
